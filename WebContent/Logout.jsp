<title>Logout</title>
</head>
<body>
<% 
if(session.getAttribute("userid")!=null)
{
	session.invalidate();
%>
  <div class="jumbotron jumbotron-fluid">
            <div class="container">
                <div class="d-flex justify-content-center">  <h1 class="display-4">Logged Out
                </h1></div>
            
                <div class="d-flex justify-content-center mt-1"><h4 class="text-center"><a href="index.jsp">Login</a> </h4>                </div>
            </div>
          </div>
<%}
else
{
	response.sendRedirect("Failure.jsp?status=sessionexpired");
}%>
